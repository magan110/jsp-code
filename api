	document.getElementById("iframeSrc").style.display = 'none';
	document.getElementById("pdfFileView").src = urlValue;
	alert(urlValue);
	*/
	pdfFilView(urlValue);
	$('#kt_modal_ComnAl_PDF').modal('show');
  }
