$(function()
{

    // Opera 8.0+
    var isOpera = (!!window.opr && !!opr.addons) || !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;

    // Firefox 1.0+
    var isFirefox = typeof InstallTrigger !== 'undefined';

    // Safari 3.0+ "[object HTMLElementConstructor]" 
    var isSafari = /constructor/i.test(window.HTMLElement) || (function (p) { return p.toString() === "[object SafariRemoteNotification]"; })(!window['safari'] || safari.pushNotification);

    // Internet Explorer 6-11
    var isIE = /*@cc_on!@*/false || !!document.documentMode;

    // Edge 20+
    var isEdge = !isIE && !!window.StyleMedia;

    // Chrome 1+
    var isChrome = !!window.chrome && !!window.chrome.webstore;
        // Blink engine detection
	$("#cusRtlFl").change(function()
	{
	//	displErr("1");
		document.DSRActv.cusRtlCd.value = '';
		custCdRtArr = "";
		cusRtlCdArr = "";
	});

	$("#areaCode").change(function()
	{
	//	displErr("1");
		document.DSRActv.cusRtlCd.value = '';
		custCdRtArr = "";
		cusRtlCdArr = "";
	});

}); 

function enabDisb() 
{
	var enabDsFl = "";
	
	if(false) 
		enabDsFl = "E";
	else
		enabDsFl = "D";

	//displErr(enabDsFl);
	//enableDisable("areaCode", enabDsFl, false, true);
	//enableDisable("cusRtlFl", enabDsFl, false, true);
//	enableDisable("docuDate", enabDsFl, false, true);
	readonly("cusRtlCd", enabDsFl, false, true);
	//document.DSRActv.cusRtlFl.value = '';
	//document.DSRActv.areaCode.value = ''; // To check Value Initialized ni displaying in Front End
	
	$("#cusRtlCd").addClass("");

	if(false) 
	{
		enabDsFl = "D";
//	displErr(1);
	//	document.DSRActv.docuDate.value = 08/07/2025;
//		enableDisable("docuDate", enabDsFl, false, false);
	}
}

function edValueKeyPress() 
{
