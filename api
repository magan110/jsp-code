function refreshed()
{
	if(document.DSRActv.dsrParam.value == "50" && document.DSRActv.dsrRem08.value != "") 
	{
		if (!mobileValid(document.DSRActv.dsrRem08, false))
		{
			document.DSRActv.dsrRem08.value = "";
			displErr("Please Enter Proper Mobile Number");
			return false;
		}
	}

	document.DSRActv.method = "POST";
	document.DSRActv.action = "DSRActv.jsp";
	document.DSRActv.submit();
}

