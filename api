function cusRtlCdHlpLoc(rqstType)
{
	var srcFldNm = "cusRtlCd";
	var cusRtlFl = document.DSRActv.cusRtlFl;
	var wherList = "";
	var cusTypFl = "";


	if(cusRtlFl.value == "")
	{
		displErr("Please select User Type");
		cusRtlFl.value = "";
		cusRtlFl.focus();
		return false;
	}

	if(cusRtlFl.value == "C" || cusRtlFl.value == "RD"  || cusRtlFl.value == "AD")
	{
		if("" == "KKR118" || document.DSRActv.dsrParam.value == "12" || document.DSRActv.dsrParam.value == "13") 
		{
			//wherList = whereAdd(wherList, "SW_28", "BL"); // Bulder Segment
			wherList = whereAdd(wherList, "SW_05", "N");
		}
		else
		{
			wherList = whereAdd(wherList, "SW_04", ""); // All Trade Segment
		}

		if(cusRtlFl.value == "C")
			cusTypFl = "T";
		else if(cusRtlFl.value == "RD")
			cusTypFl = "R";

		if("" == "KKR118" || document.DSRActv.dsrParam.value == "12" || document.DSRActv.dsrParam.value == "13") 
		{
			cusTypFl = "";
		}

		custCodeHlp('DSRActv', 'areaCode', srcFldNm, rqstType, 'O', cusTypFl, wherList);
	}
	else if(cusRtlFl.value == "R" || cusRtlFl.value == "RR")
	{
		if(cusRtlFl.value == "R")
			wherList = whereAdd(wherList, "SW_06", ""); // Exclude Subdistributor 
		else if(cusRtlFl.value == "RR")
			wherList = whereAdd(wherList, "SW_06", "S"); // Exclude Subdistributor 

		retlCodeHlp('DSRActv', srcFldNm, 'areaCode', 'A', rqstType, 'Y', wherList);
	}
	else
	{
		wherList = whereAdd(wherList, "SW_01", "Y");
		//wherList = whereAdd(wherList, "SW_04", "");
		wherList = whereAdd(wherList, "SW_07", document.DSRActv.areaCode.value);
		wherList = whereAdd(wherList, "SW_03", cusRtlFl.value);

		dataLook("DSRActv", srcFldNm, 'MK_00045', 'L', 'DN', wherList);
	
	}
	return true;
}

var custCdRtArr = "",
	cusRtlCdArr = "";

function cusRtlCdHlpget(rqstType)
{
//	displErr(rqstType);
	if(rqstType == "A")
	{
		var srcFldNm = "custCdRt",
			tgtFldNm = "custCdRtDiv",
			newCustS = "Y";
		if(!(document.DSRActv.cusRtlFl.value == "R" || document.DSRActv.cusRtlFl.value == "RR"))	
		{
			displErr("Nearest Stokiest Code Not require");
			return false;
		}
	}
	else
	{
		var srcFldNm = "cusRtlCd",
			newCustS = "",
			tgtFldNm = "cusRtlCdDiv";
	}

	var wherList = "",
		rtnValue = false,
		rqstType = "G";
		queryCod = "MK_01083";
	var cusRtlFl = document.DSRActv.cusRtlFl;

	wherList = whMasAdd(wherList, "Y");

//	wherList = whereAdd(wherList, "SW_04", "AGR");
	wherList = whereAdd(wherList, "SW_04", document.DSRActv.areaCode.value);

	if(cusRtlFl.value == "")
	{
		displErr("Please select User Type");
		cusRtlFl.value = "";
		cusRtlFl.focus();
		return false;
	}
//	displErr(newCustS);
/*	if(newCustS == "Y" && !(cusRtlFl.value == "R" || cusRtlFl.value == "RR"))
	{
		displErr("Nearest Stockiest Allow only When You select Retailer or Rural Retailer");
		cusRtlFl.value = "";
		cusRtlFl.focus();
		return false;
	}
*/
	if(document.DSRActv.areaCode.value == "")
	{
		displErr("Please select Area");
		document.DSRActv.areaCode.focus();
		return false;
	}
	
//	displErr(cusRtlFl.value);
	if(newCustS == "Y")
	{
		wherList = whereAdd(wherList, "SW_02", "'C'");
	}
	else if(cusRtlFl.value == "C")
	{
		wherList = whereAdd(wherList, "SW_08", "'PT','BL','TL','LU'");
	}
	else if(cusRtlFl.value == "RD")
	{
		wherList = whereAdd(wherList, "SW_08", "'RD'");
	}
	else if(cusRtlFl.value == "D")
	{
		if(document.DSRActv.areaCode.value == "DEL" || document.DSRActv.areaCode.value == "CAL")
		{
		//	return true
		}
		else 
		{
			document.DSRActv.cusRtlCd.value = "";
			displErr("DD Modal not applicable for your Sales Area");
			return false;
		}
		wherList = whereAdd(wherList, "SW_08", "'DD'");
	}
	else if(cusRtlFl.value == "AD")
	{
	//	displErr("A");
		wherList = whereAdd(wherList, "SW_08", "'AD','UR'");
	}
	else if(cusRtlFl.value == "DD")
	{
	//	displErr("A");
		wherList = whereAdd(wherList, "SW_08", "'DD'");
	}
	else if(cusRtlFl.value == "R")
	{
		wherList = whereAdd(wherList, "SW_06", "'R'");
	}
	else if(cusRtlFl.value == "RR")
	{
		wherList = whereAdd(wherList, "SW_07", "'R'");
	}
//	wherList = whereAdd(wherList, "SW_05", "2948");
	
//	alert(wherList);
	rtnValue = setPrmNew("DSRActv", srcFldNm, queryCod, rqstType, "DN", wherList, tgtFldNm);
	if (!rtnValue)
		return false;
//	displErr(rtnValue);
	if(newCustS == "Y")
	{
		custCdRtArr = document.getElementById(tgtFldNm).innerText.split("~~");
		document.getElementById(tgtFldNm).innerText = "";

		$("#custCdRt").autocomplete(
		{
			source: custCdRtArr
		});
	}
	else
	{
		cusRtlCdArr = document.getElementById(tgtFldNm).innerText.split("~~");
		document.getElementById(tgtFldNm).innerText = "";

		$("#cusRtlCd").autocomplete(
		{
			source: cusRtlCdArr
		});
	}
//	cusRtlCdHlpChng(cusRtlCdArr);
	return true;
}
function cusRtlCdHlpChng(currObjc)
{
//	displErr(isAndroid.matches);
	var rqstLnth = currObjc.value.split("~");
//	displErr(rqstLnth.length);

	if(rqstLnth.length == "1")
	{
//		displErr(currObjc.value);
		if(document.DSRActv.cusRtlFl.value  == "07" || document.DSRActv.cusRtlFl.value  == "08")
		{
		   document.DSRActv.cusRtlCd.value = currObjc.value;
		}
		else 
		{

		var wherList = "",
			rtnValue = false,
			rqstType = "G";
			queryCod = "MK_01083";
		var srcFldNm = "cusRtlCd",
			newCustS = "",
			tgtFldNm = "cusRtlCdDiv";
			
			wherList = whMasAdd(wherList, "Y");
			wherList = whereAdd(wherList, "SW_04", document.DSRActv.areaCode.value);
			wherList = whereAdd(wherList, "SW_09", currObjc.value);

		var rtnVal01 = setPrmNew('DSRActv', srcFldNm, queryCod, rqstType, 'DN', wherList, tgtFldNm);
/*
	wherList = whereAdd(wherList, "@__01", document.DSRActv.appRtlCd.value); 
	
	rtnKycFl = setPrmNew('DSRActv', document.DSRActv.appRtlCd.name, 'MK_01096', 'G', 'DN', wherList, 'rtnKycFlDiv');
		displErr(rtnVal01);
*/

		if (!rtnVal01)
			return false;

		custCdRtArr = document.getElementById("cusRtlCdDiv").innerText;
		
		document.getElementById("cusRtlCdDiv").innerText = "";
	///	currObjc	= custCdRtArr;
	//	document.DSRActv.cusRtlCd.value = "";
		//	return false;
		document.DSRActv.lgtitute.value = custCdRtArr.split("~")[7];
		document.DSRActv.latitute.value = custCdRtArr.split("~")[6];
		document.DSRActv.locaCapr.value = custCdRtArr.split("~")[5];
		document.getElementById("cusRtlCdDiv").innerText = custCdRtArr.split("~")[1];
		document.DSRActv.cusRtlCd.value = custCdRtArr.split("~")[0];
		}
	}
	else
	{
		document.DSRActv.lgtitute.value = currObjc.value.split("~")[7];
		document.DSRActv.latitute.value = currObjc.value.split("~")[6];
		document.DSRActv.locaCapr.value = currObjc.value.split("~")[5];
		document.getElementById("cusRtlCdDiv").innerText = currObjc.value.split("~")[1];
		document.DSRActv.cusRtlCd.value = currObjc.value.split("~")[0];
	}

	document.getElementById("cusRtlCdDiv1").innerText = document.getElementById("cusRtlCdDiv").innerText;
	var alValChk = "5";

//	   displErr(compareDateNew(document.DSRActv.docuDate.value, "17/05/2020"));
	if(compareDateNew( document.DSRActv.docuDate.value, "16/05/2020") > 0  && parseInt(withAlNm) <= parseInt(alValChk))
	{
		if(document.getElementById("lgtitute").value != "NA" && isAndroid.matches && document.getElementById("latitute").value != "NA" && document.DSRActv.dsrParam.value == "01")
		{
			var distCalc = distanceCalDsr(document.getElementById("latitute").value, document.getElementById("lgtitute").value, document.getElementById("geoLatit").value, document.getElementById("geoLongt").value, "M");
			if(distCalc < 520)
				return true;
			else
			{
				displErr("You are not at Customer`s Shop. Please Go to Purchaser Shop and retry. Your Distance from Purchaser's shop : " + distCalc);
	
				document.getElementById("cusRtlCd").value = "";
				
//				withAlNm += 1;

			//	 displErr(withAlNm);
				if(isAndroid.matches)
					getLocation();
				else
					initialise();

				return false;



			}
		}
		else if (document.DSRActv.dsrParam.value == "01" && isAndroid.matches)
		{
			displErr("Dear User Please capture retailer / Purchaser's Address First Other wise it is not allowing to update DSR Entry.");

			var openUrln = "/BirlaWhite/Mobile/LatLogCusCp.jsp?popupWin=Y&cusRtlCd=" + document.getElementById("cusRtlCd").value + "&areaCode="+document.DSRActv.areaCode.value + "&cusRtlFl=" + document.DSRActv.cusRtlFl.value;
			//createModal('Purchaser Location Capture', openUrln);
			location.href = openUrln;
			document.getElementById("cusRtlCdDiv").innerText = "";
			document.getElementById("cusRtlCd").value = "";
		}
	}
}

 function distanceCalDsr(lat1, lon1, lat2, lon2, unit) 
{
	if ((lat1 == lat2) && (lon1 == lon2)) 
	{
		return 0;
	}
	else 
	{
		var radlat1 = Math.PI * lat1/180;
		var radlat2 = Math.PI * lat2/180;
		var theta = lon1-lon2;
		var radtheta = Math.PI * theta/180;
		var dist = "";
			dist = Math.sin(radlat1) * Math.sin(radlat2) + Math.cos(radlat1) * Math.cos(radlat2) * Math.cos(radtheta);
		if (dist > 1) {
			dist = 1;
		}
		dist = Math.acos(dist);
		dist = dist * 180/Math.PI;
		dist = dist * 60 * 1.1515;
		if (unit=="K") { dist = dist * 1.609344 }
		if (unit=="M") { dist = dist * 1.609344 * 1000}
		if (unit=="N") { dist = dist * 0.8684 }
		
		return dist;
	}
}  

function cusRtlCdng(currObjc)
{
//	displErr(currObjc.value);
	var rqstLnth = currObjc.value.split("~");

	if(rqstLnth.length == "1")
	{
		displErr("Please Select Correct Code");
		document.DSRActv.custCdRt.value = "";
			return false;
	}
	//	var currObjt = currObjt.value;
	document.getElementById("custCdRtDiv").innerText = currObjc.value.split("~")[1];
	document.getElementById("custCdRt").value = currObjc.value.split("~")[0];
}

function docuDateChkLoc()
{
//	displErr(document.DSRActv.docuDate.value.substring(6,10) +"/"+ document.DSRActv.docuDate.value.substring(3,5) +"/"+ document.DSRActv.docuDate.value.substring(0,2));
//	displErr(dmyTOdmmmy(document.DSRActv.docuDate.value));
	if(!isDate(document.DSRActv.docuDate, true, "<=today"))
	{
		document.DSRActv.docuDate.value = "";
		document.DSRActv.docuDate.focus();
		return false;
	}

	var wherList = "";

	wherList = whereAdd(wherList, "@__01", "2948"); 
//	wherList = whereAdd(wherList, "@__02", document.DSRActv.docuDate.value.substring(6,10) +"/"+ document.DSRActv.docuDate.value.substring(3,5) +"/"+ document.DSRActv.docuDate.value.substring(0,2)); 
	wherList = whereAdd(wherList, "@__02", dmyTOdmmmy(document.DSRActv.docuDate.value)); 
	wherList = whereAdd(wherList, "@__03", "D"); // D for DSR

	var dataRecd = dataLook("DSRActv", "docuDate", "MK_02008", 'G', "DN", wherList);
	 

	if(document.getElementById("docuDateDiv").innerText != "OK")
	{
		displErr("Please Put Valid DSR Date. You Can submit DSR only Last Three Days. If You want to submit back date entry Please enter Exception entry(Path : Transcation --> DSR Exception Entry). Take Approval from concerned and Fill DSR Within 3 days after approval.");
		document.getElementById("docuDateDiv").innerHTML = "Please Put Valid DSR Date. You Can submit DSR only Last Three Days. If You want to submit back date entry Please enter Exception entry(Path : <a href='/BirlaWhite/Trans/GenExcp.jsp'>Click Here</a>). Take Approval from concerned and Fill DSR Within 3 days after approval.";
	//	document.DSRActv.docuDate.value = "";
		return false;
	}

	return document.getElementById("docuDateDiv").innerText;
}


function customerValid()
{
	if(!textValidApp(document.DSRActv.cusRtlFl, 'Purchaser / Retailer Flag', false))
		return false;
	
	if(!textValidApp(document.DSRActv.areaCode, 'Area Code', false))
		return false;

	if(false && !textValidApp(document.DSRActv.cusRtlCd, 'Purchaser / Retailer Code', false))  
		return false;

	return true;
}


function submitValid(submMthd)
{
	document.DSRActv.submMthd.value = submMthd;
	var msgeText = "You have choosen to Submit & " + (submMthd == "A" ? "Add" : "Exit") + ". Press OK to continue";

	if(!textValidApp(document.DSRActv.dsrParam, 'Activity Type', false))
		return false;
	
//	displErr(document.DSRActv.deptCode.value);
//	displErr(false);
	if(document.DSRActv.deptCode.value == "KKR115" && false  == true)
	{
		if(!textValidApp(document.DSRActv.FinlRslt, 'Your Address', false))
			return false;
	}
	
	if (false && !customerValid())  
		return false;

	if(document.DSRActv.dsrParam.value == "54") // In Case of Leave allowed Future Entry allowed 
	{
		if(!isDate(document.DSRActv.docuDate, false))
			return false;
	}
	else
	{
		if (!isDate(document.DSRActv.docuDate, true, "<=today"))
			return false;
	}
/*	
	if (compareDateNew(document.DSRActv.docuDate.value, "01/12/2017") < 0)
	{
		displErr("Please select Document Date from 01/12/2017");
		document.DSRActv.docuDate.focus();
		return false;
	}
*/
	if(document.DSRActv.dsrParam.value == "50") 
	{
		if (!isNumApp(document.DSRActv.dsrRem08, "PZ"))
			return false;
		if (!mobileValid(document.DSRActv.dsrRem08, false))
		{
			displErr("Please Enter Proper Mobile Number Else this is not count in Retailer Visit");
			return false;
		}
		if (!isNumApp(document.DSRActv.dsrRem09, "PZ"))
			return false;
		if (document.DSRActv.dsrRem09.value > 3)
		{
			displErr("For a singal Purchaser it can not be greater then 3 Please Enter Proper number of KYC document");
			return false;
		}
	//	displErr("document.DSRActv.dsrRem09.value" + document.DSRActv.dsrRem09.value);
	}
	var docuDtMs = docuDateChkLoc();

	if(docuDtMs != "OK")
	{
		displErr(docuDtMs);
		document.DSRActv.docuDate.focus();
		return false;
	}

	if(document.DSRActv.dsrParam.value == "41") 
	{
		if (!isNumApp(document.DSRActv.dsrRem01, "PZ"))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem02, "PZ"))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem03, "PZ"))
			return false;
	}
	if(document.DSRActv.dsrParam.value == "50" && document.DSRActv.deptCode.value == "KKR115") 
	{
		if (!isNumApp(document.DSRActv.dsrRem05, "PZ"))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem06, "PZ"))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem07, "PZ"))
			return false;
	}

	if(document.DSRActv.dsrParam.value == "31" && document.DSRActv.deptCode.value == "KKR117") 
	{
	//	displErr("document.DSRActv.dsrRem07 : " + document.DSRActv.dsrRem07.value);
		if(!textValidApp(document.DSRActv.dsrRem07, 'Contact Name', false))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem08, "PZ"))
			return false;
	}
	else if(document.DSRActv.dsrParam.value == "21" && document.DSRActv.deptCode.value == "KKR117") 
	{
	//	displErr("document.DSRActv.dsrRem07 : " + document.DSRActv.dsrRem07.value);
		if(!textValidApp(document.DSRActv.dsrRem05, 'Contact Name', false))
			return false;
		if (!isNumApp(document.DSRActv.dsrRem06, "PZ"))
			return false;
	}


	/*if(compareDateNew("08/07/2025", document.DSRActv.docuDate.value) > 3) 
	{
		displErr("Difference between Report Date and Today can not be more than 3 days");
		document.DSRActv.docuDate.focus();
		return false;
	}
	*/

	if (!txtValLBlnk(document.DSRActv.dsrRem01, "Topic Discussed", 500, true, true)) return false; 
	if (!txtValLBlnk(document.DSRActv.dsrRem02, "Ugai Recovery Plans", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem03, "Any Purchaser Grievances", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem04, "Any Other Points", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem05, "", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem05, "", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem05, "", 500, true, true)) return false;
	if (!txtValLBlnk(document.DSRActv.dsrRem05, "", 500, true, true)) return false;
	

	if(document.DSRActv.serNumbr != null)
	{
//		displErr(document.DSRActv.serNumbr.length);
		var rowLen = document.DSRActv.serNumbr.length;
		var abcLen = document.DSRActv.serNumbr.length;

		if(rowLen == null)
			rowLen = 1;

		var fldIndex = "",
			count = 0;
		var dtlDupAr = new Array();	
		var dtSrchKy = "";	

		for(var ck=0; ck<rowLen; ck++)
		{
			if(abcLen == null)
				fldIndex = document.DSRActv.serNumbr.value;
			else
				fldIndex = document.DSRActv.serNumbr[ck].value;

			if (!txtValLBlnk(eval("document.DSRActv.actnRemk" + fldIndex), "Action Remarks", 100, true, true)) 
				return false;

			if (!isNumApp(eval("document.DSRActv.prodQnty" + fldIndex), "PZ"))
				return false;

			if (!isNumApp(eval("document.DSRActv.projQnty" + fldIndex), "PZ"))
				return false;
			if("" == "01")
			{

			if (!txtValLBlnk(eval("document.DSRActv.repoDesc" + fldIndex), "Prodcut Code", 100, true, true)) 
				return false;
			if (!txtValLBlnk(eval("document.DSRActv.catgPack" + fldIndex), "Prodcut Code", 100, true, true)) 
				return false;
	//			displErr(document.getElementById("repoDesc" + fldIndex + "Div").innerText);
				//repoDescPop
				dtSrchKy = eval("document.DSRActv.catgPack" + fldIndex).value;
				if (isDupDtlVal(dtlDupAr, "document.DSRActv.catgPack" + fldIndex, dtSrchKy))
				{
					displErr("Please Check Duplicate Invoice. Product Code : " + document.getElementById("repoDesc" + fldIndex + "Div").innerText);
					return false;
				}
				dtlDupAr[ck] = dtSrchKy;
			}
		}
	}

//	displErr("document.DSRActv.dsrRem09.value" + document.DSRActv.dsrRem09.value);
	
	if(document.DSRActv.dsrParam.value == "50") 
	{
		if(!textValidApp(document.DSRActv.district, 'District Name', false))
			return false;
		if(!textValidApp(document.DSRActv.pinCodeN, 'Pin Name', false))
			return false;
		if(!textValidApp(document.DSRActv.cityName, 'City Name', false))
			return false;
		if(!textValidApp(document.DSRActv.cstBisTy, 'Counter Type', false))
			return false;

		if(document.DSRActv.cusRtlFl.value == "R" || document.DSRActv.cusRtlFl.value == "RR")
		{
			if(!textValidApp(document.DSRActv.custCdRt, 'Nearest Stockiest', false))
			return false;
		}

	}
/*	
	if(document.DSRActv.dsrParam.value == "50" || document.DSRActv.dsrParam.value == "01") 
	{
		if(document.DSRActv.serNumMk != null)
		{
			var rowLen1 = document.DSRActv.serNumMk.length;
			var abcLen1 = document.DSRActv.serNumMk.length;

			if(rowLen1 == null)
				rowLen1 = 1;

			var fldIndex1 = "";

			for(var i=0; i<rowLen1; i++)
			{
				if(abcLen == null)
					fldIndex1 = document.DSRActv.serNumMk.value;
				else
					fldIndex1 = document.DSRActv.serNumMk[i].value;
	
				if (!isNumApp(eval("document.DSRActv.prodQntyMk" + fldIndex1), "PZ"))
					return false;

				if (!isNumApp(eval("document.DSRActv.projQntyMk" + fldIndex1), "PZ"))
					return false;
			}
		}
	}


	if (count == 0)
	{
		displErr("Please enter atleast 1 Detail to Save");
		return false;
	}
*/

	if (!subValidCom("DSRActv"))
		return false;


	displSav(msgeText);
	return false;
}

function frmSubmt(val) // For Common Submit Function 
{
//	enableDisable("cityName", "E", false, false);
//	enableDisable("district", "E", false, false);

//	document.DSRActv.docuDate.value = dmyTOdmmmy(document.DSRActv.docuDate.value);

	if (val)
	{
		document.DSRActv.method = "POST";
		document.DSRActv.action = "DSRActvS.jsp"; 
		document.DSRActv.submit();
	}
} 

var lat, lng, address;

function initiate_geolocation() 
{
	navigator.geolocation.getCurrentPosition(handle_geolocation_query);
}

function handle_geolocation_query(position)
{
	lat = position.coords.latitude;	//To
	document.getElementById("geoLatit").value = lat; 
	lng = position.coords.longitude;
	document.getElementById("geoLongt").value = lng;
}

function GetAddress() 
{
var latlng = new google.maps.LatLng(lat, lng);
var geocoder = geocoder = new google.maps.Geocoder();

geocoder.geocode({ 'latLng': latlng }, function (results, status) 
{
	if (status == google.maps.GeocoderStatus.OK) 
	{
		if (results[1]) 
		{
			address = results[1].formatted_address;
			document.getElementById("FinlRslt").value = address; 
			$("#finlRsltDiv").show();
		}
	}
	else 
	{
		$("#finlRsltDiv").hide();
	//	displErr("Geocoder failed due to: " + status + " Please Enable Location");
	}

});
}

if(false  == true)
{
	$(function()
	{
	initiate_geolocation();

	setTimeout(function(){
	  GetAddress();
	}, 3000);

	});	
}

$("#messgae").click();
