		m = insertCellBw(newRow);
		m.innerHTML = prodQntyPr; 

		m = insertCellBw(newRow);
		m.innerHTML = projQntyPr; 

		m = insertCellBw(newRow);
		m.innerHTML = mrtkDatePr; 
}

