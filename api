	}
	
	emplCodeLuk('<%=formName%>', 'concEmpl', 'L', 'B', '', '',  "<%=WebSessBean.resArStrGet()%>", "");

 	/*
	}
	else
	{
		alert("Currently Concern Employee updation option is disabled.");
		return false;
	}
	*/
}   
